{
  "name": "album",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "albumid": {
      "type": "number",
      "required": true
    },
    "albumname": {
      "type": "string",
      "required": true
    },
    "userid": {
      "type": "number",
      "required": true
    },
    "isactive": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "createddate": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "photos": {
      "type": "hasMany",
      "model": "Photo",
      "foreignKey": "albumid",
      "through": "PhotosInAlbum"
    }
  },
  "acls": [],
  "methods": {}
}
