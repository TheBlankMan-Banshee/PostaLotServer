{
  "name": "Photo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": false
    },
    "userid": {
      "type": "string",
      "required": true
    },
    "storageurl": {
      "type": "string",
      "required": true
    },
    "isactive": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "postdate": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "userphoto": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userid"
    },
    "photosInAlbums": {
      "type": "hasAndBelongsToMany",
      "model": "PhotosInAlbum",
      "foreignKey": ""
    },
    "metadata": {
      "type": "hasOne",
      "model": "user",
      "foreignKey": "userid"
    }
  },
  "acls": [],
  "methods": {}
}
