  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "Photo.getViewablePhotos":{
      "accepts":[
        {"arg": "userid", "type":"number"}
      ],
      "returns":[],
      "http": {
        "verb":"get",
        "path":"/findAll",
